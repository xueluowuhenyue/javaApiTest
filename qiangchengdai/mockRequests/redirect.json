[
  {
    "description": "请求重定向到百度",
    "request": {
      "uri": "/redirect"
    },
    "redirectTo": "http://www.baidu.com"
  },

  {
    "description": "请求重定向到自定义地址",
    "request": {
      "uri": "/redirect/path"
    },
    "redirectTo": "/redirect/path/new"
  },
  {
    "description": "这是被重定向的地址",
    "request": {
      "uri": "/redirect/path/new"
    },
    "response": {
      "text": "重定向成功了",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]